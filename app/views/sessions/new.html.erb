<div class="login-container">
  <h1>Log In</h1>
  <%= form_with url: login_path, method: :post do |form| %>
    <%= form.label :username %>
    <%= form.text_field :username %>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <%= form.submit "Log In" %>
  <% end %>
  <p>New explorer? <%= link_to "Sign up", signup_path %></p>
</div>
